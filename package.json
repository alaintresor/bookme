{
  "name": "bookme",
  "version": "1.0.0",
  "description": "Make company global travel and accommodation easy and convenient for the strong workforce of savvy members of staff, by leveraging the modern web.",
  "main": "src/index.js",
  "type":"module",
  "scripts": {
    "start": "cross-env NODE_ENV=production node --exec babel-node ./src/index.js",
    "start:prod": "cross-env NODE_ENV=production nodemon --exec babel-node ./src/index.js",
    "start:dev": "cross-env NODE_ENV=development nodemon --exec babel-node ./src/index.js",
    "migrate": "npx sequelize-cli db:migrate",
    "down": "npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "undo-seed": "npx sequelize-cli db:seed:undo",
    "test": "cross-env NODE_ENV=test  npm run down && npm run migrate && npm run seed && mocha --require @babel/register --timeout 30000 --exit",
    "coverage": "nyc npm run test | coveralls",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "postinstall": "husky install",
    "lint": "eslint",
    "lint:fix": "npm run lint --fix",
    "prettier": "npx prettier src test --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "lint:watch": "npm run lint -- --watch"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js",
      "test/*.test.js"
    ],
    "reporter": [
      "html",
      "text",
      "text-lcov"
    ],
    "exclude": [
      "node_modules/*",
      "src/database/seeders/*",
      "src/index.js",
      "coverage/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atlp-rwanda/bravo-bn-be.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/atlp-rwanda/bravo-bn-be/issues"
  },
  "homepage": "https://github.com/atlp-rwanda/bravo-bn-be#readme",
  "dependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/compat-data": "^7.17.10",
    "@babel/node": "^7.17.10",
    "@babel/polyfill": "^7.12.1",
    "@babel/register": "^7.17.7",
    "@babel/runtime": "^7.17.9",
    "astral-regex": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "babel-node": "^0.0.1-security",
    "babel-register": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "cloudinary": "^1.30.0",
    "cloudinary-multer": "^1.0.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "human-signals": "^4.1.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "lint": "^0.7.0",
    "lint-fix": "^0.24.0",
    "lint-staged": "^13.0.0",
    "multer": "^1.4.4-lts.1",
    "multer-gridfs-storage": "^5.0.2",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemailer": "^6.7.5",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth2": "^0.2.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "prettier": "^2.6.2",
    "redis": "^4.2.0",
    "redis-connection": "^5.4.0",
    "sequelize": "^5.22.5",
    "socket.io": "^4.5.0",
    "socket.io-client": "^4.5.1",
    "swagger-ui-express": "^4.4.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-transform-runtime": "^7.17.12",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-istanbul": "^6.1.1",
    "coveralls": "^3.1.1",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^8.0.1",
    "mocha": "^10.0.0",
    "mocha-lcov-reporter": "^1.3.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.16",
    "nyc": "^15.1.0",
    "sequelize-cli": "^5.5.1"
  },
  "lint-staged": {
    "*.{js,css,md,ts, tsx}": "prettier --write"
  }
}
